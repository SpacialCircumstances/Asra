let
    `==` = fun a b -> true
    (`-`: Int -> Int -> Int) = fun a b -> a
    (`+`: Int -> Int -> Int) = fun a b -> b
    rec fib = fun n -> 
        if (n == 1) then 0 else (if (n == 2) then 1 else (
            let 
                f = (fib (n - 1))
                g = (fib (n - 2))
            in f + g end) end) end
in fib 3 end